<?xml version="1.0" encoding="UTF-8"?>
<Configuration package="gov.vha.isaac.ochre.workflow"
               status="INFO">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="gov.vha.isaac.ochre.integration.tests.IntegrationSuiteManagement" level="info"/>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>